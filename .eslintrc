{
  "extends": "eslint-config-airbnb",
  "rules": {
    "import/prefer-default-export": 0,
    "arrow-body-style": 0,
    "arrow-parens": 0
    },
  "env": {
    "jest": true,
    "browser": true
  },
  "settings": {
    "import/resolver": {
      "babel-module": {
        "extensions": [".js", ".jsx"]
      }
    }
  }
}
